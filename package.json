{
  "name": "jawad",
  "version": "1.0.0",
  "private": true,
  "description": "Jawad - Bring Your Own AI to Every Website. A Browser Operating System.",
  "scripts": {
    "build": "node build.mjs",
    "lint": "tsc --noEmit",
    "package": "node build.mjs && npx web-ext build --source-dir=dist --artifacts-dir=web-ext-artifacts --overwrite-dest",
